/*动态添加图片路径*/
        var urls = document.getElementsByClassName("icon-add")
        for (var i = 0; i < urls.length; i++) {
            var url = urls[i].host
            var imgUrl = "http://" + url + "/favicon.ico"
            var img = urls[i].getElementsByTagName("img")
            img[0].setAttribute("src", imgUrl)
        }